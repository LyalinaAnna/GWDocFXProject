{
  "metadata": [
    {
      "src": [
        {
          "src": "./../",
          "files": [
            "VR Trainers GWPro_Work/Assets/Trainers/**.cs",
            "VR Trainers GWPro_Work/Assets/VRCommonComponents/**.cs"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": true
    }    
  ],
  "build": {
    "cleanupCacheHistory": false,
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "toc.yml",
          "*.md"
        ]
      },      
      {
        "files": [
          "developmentPipline/**"
        ]
      },
      {
        "files": [
          "simulatorDevelopmentGuide/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "GWDoc",
      "_appFaviconPath": "./Images/favicon.ico",
      "_appLogoPath": "./Images/logo.svg"
    },
    "dest": "_site",
    "disableGitFeatures": false,
    "fileMetadataFiles": [],    
    "keepFileLink": false,
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "template": [
      "default"
    ]
  }
}